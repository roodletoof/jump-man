mouse = {}
mouse.enabled = true
mouse.stats = {}
mouse.stats.x = 32
mouse.stats.y = 33
mouse.stats.inp = 34
//-> button bitmask (1=primary, 2=secondary, 4=middle)

//has won?
victory = false

function enable_mouse()
  mouse.enabled = true
end

function disable_mouse()
  mouse.enabled = false
end

menuitem(1, "enable mouse", enable_mouse)
menuitem(2, "disable mouse", disable_mouse)

//objects
objects = {}
objects.list = {}
//objects currently in scene
objects.active = {}

//controls

controls 			= {}
controls.⬅️ = 0
controls.➡️ = 1
controls.⬆️ = 2
controls.⬇️ = 3
controls.a 	= 4
controls.b 	= 5

function _init()
	//camera pan amount
	pan_offz = make_vector()

	//mouse
	mouse.point = make_vector()
	//reticle
	reticle = {}
	reticle.position = make_vector()
	reticle.raw_position = make_vector()
	reticle.origin = make_vector()
	reticle.sprite = 16
	reticle.offset = -1.5
	reticle.radius = 12
	
	
	player = make_object("player",1,1,2,0.8,10)
	player.jump_pressed_prev = false
	
	
	box = make_object("box",32,1,1,0.8,1)

	replace_tiles_with_objects()
end

function _update60()
	
	if not victory then
		for o in all(objects.active) do
			
			o.velocity.y = gravity(o.velocity.y)
			
			o = move_and_slide(o,true)
			
			o.grounded = is_grounded(o)
			
			if o.name == "player" then
				player_update(o)
			end
		end
	end
end

function _draw()
	cls()
	map_data.draw_position.x = map_data.startpoint.x * 8 - 8*8
	map_data.draw_position.y = map_data.startpoint.y * 8 - 8*8
	map(map_data.startpoint.x - 8, map_data.startpoint.y - 8, map_data.draw_position.x, map_data.draw_position.y, map_data.size.w + 16, map_data.size.h + 16)
	for o in all(objects.active) do
		
		spr(o.sprite,o.position.x,o.position.y,o.w,o.h)
		
	end
	
	//draw reticle and line
	if mouse.enabled then
		spr(reticle.sprite,stat(mouse.stats.x) + cam_offset.x - 1, stat(mouse.stats.y) + cam_offset.y - 1)
	else
		spr(reticle.sprite,reticle.position.x+reticle.offset,reticle.position.y+reticle.offset)
	end
	
	if future_pos != nil then
		for pos = 1,59 do
			
			draw_line(future_pos[pos], future_pos[pos+1])
			
		end
	end
	
	if victory then
		for s = 4,10 do
			local x_space = (s - 4) * 8
		 spr(s,cam_offset.x + 35 + x_space,cam_offset.y + 60)
		end
	end
	
end
