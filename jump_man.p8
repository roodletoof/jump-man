--player
function player_update(o)
	//victory?
	if is_flag_8x8(o.position,flags.goal) then
		if not victory then
			sfx(1)
		end
		victory = true
	end
	
	//switching screens
	if o.position.x+4 > (map_data.startpoint.x + map_data.size.w) * 8 then
		map_data.startpoint.x += 16
		replace_tiles_with_objects()
	elseif o.position.x+4 < map_data.startpoint.x * 8 then
		map_data.startpoint.x -= 16
		replace_tiles_with_objects()
	end
	if o.position.y+4 > (map_data.startpoint.y + map_data.size.h) * 8 then
		map_data.startpoint.y += 16
		replace_tiles_with_objects()
	elseif o.position.y+4 < map_data.startpoint.y * 8 then
		map_data.startpoint.y -= 16
		replace_tiles_with_objects()
	end
	
	
	local wasd = make_vector()
	wasd.x = (int(btn(controls.➡️,1)) - int(btn(controls.⬅️,1))) * 4 * 8
	wasd.y = (int(btn(controls.⬇️,1)) - int(btn(controls.⬆️,1))) * 4 * 8
	pan_offz.x = move_towards(pan_offz.x, wasd.x, 5)
	pan_offz.y = move_towards(pan_offz.y, wasd.y, 5)
	
	//cam_offset = make_vector((o.position.x-8*8)+4,(o.position.y-8*8)+4)
	//cam_offset = make_vector((o.position.x-8*8)+4,(o.position.y-8*8)+4)
	cam_offset = make_vector(map_data.startpoint.x*8+pan_offz.x,map_data.startpoint.y*8+pan_offz.y)
	camera(cam_offset.x,cam_offset.y)
	
	
	//table filled with 60 predicted
	//future positions
	future_pos = nil
	
	//animation
	if o.grounded then
		o.sprite = 1
	else
		o.sprite = 3
	end
	
	//is jump being pressed
	if btn(controls.a) or stat(mouse.stats.inp) == 00000001 then
		o.jump_pressed = true
	else
		o.jump_pressed = false
	end 
	
	if o.jump_pressed and is_grounded(o) then
		o.jump_pressed_prev = true
		o.sprite = 2
		if o.speed <= o.speed_max then
			o.speed += 1/60*o.speed_max
		end
		
		future_pos = {}
		local vir_o = deepcopy(o)
		
		vir_o.velocity = get_velocity(o.speed)
		//predicting future positions
		//and storing them in future_pos
		for f = 1,60 do
			vir_o.velocity.y = gravity(vir_o.velocity.y) 
			vir_o = move_and_slide(vir_o)
			local adjusted_position = deepcopy(vir_o.position)
			adjusted_position.x += 4
			adjusted_position.y += 4
			add(future_pos, adjusted_position)
		end
		
	end
	
	//jumps when released
	if o.jump_pressed_prev and not o.jump_pressed and is_grounded(o) then
		o.velocity = get_velocity(o.speed)
		o.jump_pressed_prev = false
		o.speed = 1.0
	end
	
	if o.jump_pressed_prev and not o.jump_pressed then
		o.jump_pressed_prev = false
	end
	
	//arrow keys logic
	if mouse.enabled == false then
	
		local x_dir = int(btn(controls.➡️)) - int(btn(controls.⬅️))
		
		o.aim_deg += x_dir * -1
		
		if o.aim_deg == -1 then
			o.aim_deg = 359
		elseif o.aim_deg == 360 then
			o.aim_deg = 0
		end
		
		local angle = 1/360*o.aim_deg
		
		reticle.origin.x = o.position.x + 4
		reticle.origin.y = o.position.y + 4
		reticle.raw_position.x = reticle.radius*cos(angle)
		reticle.raw_position.y = reticle.radius*sin(angle)
		reticle.position.x = reticle.raw_position.x + reticle.origin.x
		reticle.position.y = reticle.raw_position.y + reticle.origin.y
		reticle.raw_position.x = reticle.raw_position.x/reticle.radius
		reticle.raw_position.y = reticle.raw_position.y/reticle.radius
	else
		//mouse logic
		
		//resets the position data to a point around 0
		local origin_point = deepcopy(o.position)
		origin_point.x += 4
		origin_point.y += 4
		mouse.point = make_vector(stat(mouse.stats.x) + cam_offset.x, stat(mouse.stats.y) + cam_offset.y)
		mouse.point.x = mouse.point.x - origin_point.x
		mouse.point.y = mouse.point.y - origin_point.y
		//calculate length/magnitude
		local magnitude = sqrt(mouse.point.x*mouse.point.x+mouse.point.y*mouse.point.y)
		if magnitude != 0 then
			reticle.raw_position.x = mouse.point.x / magnitude
			reticle.raw_position.y = mouse.point.y / magnitude
		end
	end
end
