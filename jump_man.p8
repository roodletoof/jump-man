--functions

function make_object(name,sprite,w,h,bouncy_scale,speed)
	local obj = {}
	obj.position = make_vector()
	obj.velocity = make_vector()
	obj.sprite = sprite or 1
	obj.w = w or 1
	obj.h = h or 1
	obj.name = name or "player"
	obj.bouncy_scale = bouncy_scale or 0.0
	obj.speed = 1.0
	obj.speed_max = speed or 1.0
	obj.aim_deg = 0
	obj.grounded = false
	obj.friction = 0.5
	
	objects.list[obj.sprite] = obj
	//add(objects.list, obj, obj.sprite)
	
	return obj
end

//physics
function gravity(vel_y)
	local new_vel_y
	
	if vel_y <= constant.max_speed then
		new_vel_y = vel_y + constant.gravity
	else
		new_vel_y = vel_y
	end
	
	return new_vel_y
end

function move_and_slide(object,complex)
	//run all functions?
	complex = complex or false
	//checs if object is inside
	//water and stuff
	if complex then
		//vector for force direction
		//applied to object
		local force_dir = make_vector()
		force_dir.x = int(is_flag_8x8(object.position,flags.force.r)) - int(is_flag_8x8(object.position,flags.force.l))
		force_dir.y = int(is_flag_8x8(object.position,flags.force.d)) - int(is_flag_8x8(object.position,flags.force.u))
		
		local force_amount = constant.water_strength
		
		if abs(object.velocity.x) <= constant.max_speed then
			object.velocity.x += force_dir.x*force_amount
		end
		
		if  abs(object.velocity.y) <= constant.max_speed then
			object.velocity.y += force_dir.y*force_amount
		end
	end
	
	//basic collision
	object.position.x += object.velocity.x
	while is_flag_8x8(object.position) do
		object.position.x -= pos_or_neg(object.velocity.x) * 0.1
		if not is_flag_8x8(object.position) then
			object.velocity.x = object.velocity.x * object.bouncy_scale * -1
			if abs(object.velocity.x) > 0.8 and complex then
				sfx(0)
			end
		end
	end
	
	if is_grounded(object) then
		//object.velocity.x = move_towards(object.velocity.x,0.0,object.friction)
		object.velocity.x = lerp(object.velocity.x)
	end
	
	object.position.y += object.velocity.y
	while is_flag_8x8(object.position) do
		object.position.y -= pos_or_neg(object.velocity.y) * 0.1
		if not is_flag_8x8(object.position) then
			object.velocity.y = object.velocity.y * object.bouncy_scale * -1
			if abs(object.velocity.y) > 0.8 and complex then
				sfx(0)
			end
		end
	end
	
	return object
	
end

function is_grounded(o)
	
	local modified_pos = deepcopy(o.position)
	modified_pos.y += 1
	
	if is_flag_8x8(modified_pos,flags.solid) then
		return true
	else
		return false
	end
end

function move_towards(from,to,amount)
	if to < from then
		return max(to, from - amount)
	elseif to > from then
		return min(to, from + amount)
	else
		return from
	end
end

function lerp(from, amount)
	from = from * 0.9
	return from
end

function pos_or_neg(i)
	if i > 0 then
		return 1
	elseif i < 0 then
		return -1
	else
		return 0
	end
end

function int(bool)
	if bool then
		return 1
	else
		return 0
	end
end

//copies over table
function deepcopy(orig)
    local orig_type = type(orig)
    local copy
    if orig_type == 'table' then
        copy = {}
        for orig_key, orig_value in next, orig, nil do
            copy[deepcopy(orig_key)] = deepcopy(orig_value)
        end
        setmetatable(copy, deepcopy(getmetatable(orig)))
    else -- number, string, boolean, etc
        copy = orig
    end
    return copy
end

function draw_line(vec1,vec2,c)
	c = c or 7
	line(vec1.x, vec1.y, vec2.x, vec2.y, c)
end

function make_vector(x,y)
	local vector = {}
	vector.x = x or 0.0
	vector.y = y or 0.0
	return vector
end

//is point colliding?
function is_flag_p(position,flag)
	flag = flag or 0
	if fget(mget(position.x/8, position.y/8), flag) then
		return true
	else
		return false
	end
end

//are any corners of a 8x8 box colliding?
function is_flag_8x8(position,flag)
	flag = flag or 0
	local offset = 7
	if is_flag_p(position,flag)
	or is_flag_p({["x"] = position.x + offset, ["y"] = position.y},flag)
	or is_flag_p({["x"] = position.x, ["y"] = position.y + offset},flag)
	or is_flag_p({["x"] = position.x + offset, ["y"] = position.y + offset},flag) then
		return true
	else
		return false
	end
end

//spawning in objects
function replace_tiles_with_objects()
	for x = map_data.startpoint.x, map_data.startpoint.x + map_data.size.w do
		for y = map_data.startpoint.y, map_data.startpoint.y + map_data.size.h do
			
			local obj = objects.list[mget(x,y)]
			
			if obj != nil then
				local clone = deepcopy(obj)
				add(objects.active, clone)
				objects.active[#objects.active].position.x = x * 8
				objects.active[#objects.active].position.y = y * 8
				mset(x,y,0)
			end
			
		end
	end

end

//reticle velocity
function get_velocity(speed)
	local velocity = make_vector()
	velocity.x = reticle.raw_position.x*speed
	velocity.y = reticle.raw_position.y*speed
	return velocity
end
